{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "no-console": "off",

      "no-underscore-dangle": ["error", {"allow": ["_id"]}],

      "arrow-body-style": ["error", "always"]

    }
}
